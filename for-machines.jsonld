              "name": "Before Your First Commit",
              "itemListElement": [
                {"@type": "HowToStep", "text": "Create .env with all credentials"},
                {"@type": "HowToStep", "text": "Add .env to .gitignore"},
                {"@type": "HowToStep", "text": "Verify git status (should NOT show .env)"},
                {"@type": "HowToStep", "text": "Replace hardcoded credentials with os.getenv()"}
